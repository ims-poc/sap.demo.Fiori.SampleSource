<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<HBox width="100%">
		<Label text="{i18n>LIST_PAGE_FILTER_TEXT}" class="sapUiSmallMarginTop sapUiSmallMarginBottom" />
		<FacetFilter
			id="confirmChangePriceFctFlt"
			type="Simple"
			lists="{tblData>/Filters}"
			liveSearch="true"
			showPersonalization="false"
			showReset="true"
			reset="onFacetFilterReset">
			<lists>
				<FacetFilterList
					title="{tblData>Category}"
					growing="false"
					items="{tblData>values}"
					key="{tblData>FilterKey}"
					listOpen="onFacetFilterListOpen"
					listClose="setFilter">
					<items>
						<FacetFilterItem
							key="{tblData>data}"
							text="{tblData>text}"
						/>
					</items>
				</FacetFilterList>
			</lists>
		</FacetFilter>
	</HBox>
</core:FragmentDefinition>